{
  "name": "vulgar",
  "version": "2.0.4",
  "description": "A simple and scalable MEAN stack development kit featuring Angular 2 (Router, Http, Forms, Services, Tests, E2E, Coverage, Dev/Prod), Express, MongoDB, Mongoose, Node, Redux, @ngrx/store,  PassportJS, Socket.io, Karma, Protractor, Jasmine, Istanbul, SASS Support, TypeScript, TSLint, NG2Lint, Hot Module Replacement, Docco, Gulp, and Webpack by @datatype_void",
  "keywords": [
    "angular2",
    "webpack",
    "typescript",
    "node",
    "express",
    "mongodb"
  ],
  "author": "David Newman <david.r.niciforovic@gmail.com>",
  "homepage": "https://github.com/datatypevoid/vulgar",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --config config/webpack/client.dev.js --progress --profile",
    "build:docker": "npm run build:prod && docker build -t angular2-webpack-start:latest .",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack/client.prod.js  --progress --profile --bail",
    "ci": "npm run lint && npm test && npm run e2e",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "clean:dist": "npm run rimraf -- dist/client",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/ ./config/",
    "docker": "docker",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "fix:line-endings": "find . \\( -type f -and -not -path '*/node_modules/*' \\) -exec dos2unix {} \\;",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "lint:client": "npm run tslint \"src/client/**/*.ts\"",
    "lint:server": "npm run tslint \"src/server/**/*.ts\"",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack/client.dev.js --progress --profile --watch --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "start": "concurrently --kill-others --raw \"npm run server:dev\" \"npm run watch\"",
    "test": "karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "webdriver-manager": "webdriver-manager",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
